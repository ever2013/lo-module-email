{
  "name": "loveorigami/lo-module-email",
  "description": "Email module for Lo-CMS",
  "version": "2.1",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "email",
    "module"
  ],
  "homepage": "https://github.com/loveorigami/lo-module-email",
  "time": "2016-10-27",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrey Lukyanov",
      "email": "loveorigami@mail.ru",
      "homepage": "http://loveorigami.info",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1",
    "yiisoft/yii2": "~2",
    "guzzlehttp/guzzle": "~6.2",
    "php-http/guzzle6-adapter": "1.1.1",
    "sparkpost/sparkpost": "2.1.0"
  },
  "autoload": {
    "psr-4": {
      "lo\\modules\\email\\": "src/"
    }
  },
	"extra": {
		"config-plugin": {
			"admin": [
				"src/modules/admin/config/main.php"
			],
			"web": [
				"src/config/main.php"
			]
		}
	}
}
